{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-no-circular-imports"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      false
    ],
    "max-line-length": [
      false
    ],
    "no-empty-interface": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "restrict-plus-operands": true
  },
  "rulesDirectory": []
}
